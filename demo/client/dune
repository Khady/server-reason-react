(melange.emit
 (alias client)
 (target app)
 (promote (until-clean))
 (modules app)
 (libraries melange shared_js reason-react)
 (preprocess
  (pps reason-react-ppx browser_ppx -js melange.ppx))
 (module_systems commonjs))

