(library
 (name melstd)
 (public_name server-reason-react.melstd)
 (modules hash_gen hash_set hash_set_gen hash_set_string map_gen map_string melstd set_gen set_string string)
 (libraries unix pp))

(rule
 (targets hash_set_string.ml)
 (deps hash_set.cppo.ml)
 (action
  (run cppo -D TYPE_STRING %{deps} -o %{targets})))

